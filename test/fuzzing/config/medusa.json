{
	"fuzzing": {
		"workers": 8,
		"shrinkLimit": 5000,
		"callSequenceLength": 30,
		"corpusDirectory": "medusa_assert_corpus",
		"targetContracts": ["FuzzingSuite"],
		"deployerAddress": "0x10000",
		"senderAddresses": [
			"0x20000"
		],
		"testing": {
			"assertionTesting": {
				"enabled": true
			},
			"propertyTesting": {
				"enabled": false
			},
			"optimizationTesting": {
				"enabled": false
			}
		},
		"chainConfig": {
			"codeSizeCheckDisabled": true,
			"cheatCodes": {
				"enableFFI": true
			}
		}
	},
	"compilation": {
		"platform": "crytic-compile",
		"platformConfig": {
			"args": [
				"--compile-force-framework", "foundry",
				"--compile-libraries=(DoubleEndedQueue,0xf0),(HugeUint,0xf1),(TickMath,0xf2),(UsdnProtocolCoreLibrary,0xf3),(UsdnProtocolActionsUtilsLibrary,0xf4),(UsdnProtocolActionsVaultLibrary,0xf5),(UsdnProtocolVaultLibrary,0xf6),(UsdnProtocolLongLibrary,0xf7),(UsdnProtocolActionsLongLibrary,0xf8)",
				"--foundry-compile-all"
			],
			"exportDirectory": "../../../crytic-export",
			"solcVersion": "0.8.26",
			"target": "../../../."
		}
	},
	"logging": {
		"level": "info",
		"logDirectory": ""
	}
}