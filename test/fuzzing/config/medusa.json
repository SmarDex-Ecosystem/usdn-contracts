{
	"fuzzing": {
		"workers": 8,
		"shrinkLimit": 5000,
		"callSequenceLength": 30,
		"corpusDirectory": "medusa_assert_corpus",
		"targetContracts": ["FuzzingSuite"],
		"deployerAddress": "0x10000",
		"senderAddresses": [
			"0x20000"
		],
		"testing": {
			"assertionTesting": {
				"enabled": true
			},
			"propertyTesting": {
				"enabled": false
			},
			"optimizationTesting": {
				"enabled": false
			}
		},
		"chainConfig": {
			"codeSizeCheckDisabled": true,
			"cheatCodes": {
				"enableFFI": true
			}
		}
	},
	"compilation": {
		"platform": "solc",
		"platformConfig": {
			"args": ["--compile-force-framework", "foundry"],
			"solcVersion": "0.8.26",
			"target": "../FuzzingSuite.sol"
		}
	},
	"logging": {
		"level": "info",
		"logDirectory": ""
	}
}